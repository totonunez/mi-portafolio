generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Ciudadano {
  id             Int     @id @default(autoincrement())
  edad           Int
  genero         String
  educacion      String
  estadoCivil    String
  hijos          Int
  religion       String
  ocupacion      String
  tipoContrato   String
  creadoEn       DateTime @default(now())
}